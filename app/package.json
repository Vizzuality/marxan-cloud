{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "storybook:styles": "tailwind build ./styles/tailwind.css -o ./public/styles/tailwind.storybook.css",
    "storybook:dev": "yarn storybook:styles && start-storybook -p 6006 -s ./public -c .storybook",
    "storybook:build": "yarn storybook:styles && build-storybook -s ./public -c .storybook"
  },
  "dependencies": {
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-essentials": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/react": "^6.1.9",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.2.2",
    "cypress": "^6.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "postcss": "^8.1.10",
    "prettier": "^2.2.0",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
