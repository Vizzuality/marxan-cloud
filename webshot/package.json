  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "express": "4.17.2",
    "helmet": "5.0.2",
    "puppeteer": "13.3.2",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "bunyan": "1.8.15",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-node": "10.5.0",
    "tsconfig-paths": "3.12.0"
  }
