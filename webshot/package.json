{
  "name": "@marxan-cloud/webshot",
  "version": "1.2.15.0",
  "description": "Webshot service",
  "main": "main.js",
  "scripts": {
    "start": "node dist/main.js",
    "prestart:prod": "rimraf dist && tsc",
    "start:dev": "nodemon --exec 'ts-node --files --project tsconfig.json -r tsconfig-paths/register src/main.ts' -L | bunyan",
    "start:debug": "nodemon --exec 'NODE_OPTIONS=\"--inspect=0.0.0.0\" ts-node --files --project tsconfig.json -r tsconfig-paths/register src/main.ts' -L | bunyan",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "~18.17"
  },
  "author": "Vizzuality",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "config": "3.3.8",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "puppeteer": "20.4.0",
    "typescript": "^4.9"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "bunyan": "1.8.15",
    "nodemon": "2.0.20",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1"
  },
  "resolutions": {
    "@types/node": "18.17.3"
  },
  "volta": {
    "node": "18.17.0"
  }
}
