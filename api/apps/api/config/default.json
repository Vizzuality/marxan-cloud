{
  "network": {
    "cors": {
      "origins": [
        "http://localhost:3010"
      ]
    }
  },
  "redisApi": {
   "connection": {
     "host": "marxan-redis",
     "port": 6379
     },
   "concurrency": 50
   },
  "api": {
    "url": "http://api:3000"
  },
  "jobOptions":{
    "removeOnComplete": 100,
    "removeOnFail": 1000,
    "attempts": 3,
    "timeout": 60000
  },
  "auth": {
    "jwt": {
      "expiresIn": "2h"
    }
  },
  "geoprocessing": {
    "url": "http://geoprocessing:3000"
  },
  "storage": {
    "sharedFileStorage": {
      "localPath": "/tmp/storage"
    }
  },
  "marxan": {
    "inputFiles": {
      "inputDat": {
        "ioSettings": {
          "INPUTDIR": "input",
          "PUNAME": "pu.dat",
          "SPECNAME": "spec.dat",
          "PUVSPRNAME": "puvspr.dat",
          "BOUNDNAME": "bound.dat",
          "OUTPUTDIR": "output"
        }
      }
    }
  },
  "fileUploads": {
    "limits": {
      "shapefileMaxSize": 10240000
    }
  },
  "sparkpost": {
    "apikey": "invalidSparkpostApikey",
    "origin": "https://api.eu.sparkpost.com:443"
  },
  "passwordReset": {
    "tokenPrefix": "http://localhost:3000/auth/reset-password?token=",
    "expiration": 1800000
  }
}

