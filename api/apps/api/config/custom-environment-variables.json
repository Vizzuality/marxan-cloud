{
  "logging": {
    "muteAll": "API_LOGGING_MUTE_ALL"
  },
  "postgresApi": {
    "host": "API_POSTGRES_HOST",
    "username": "API_POSTGRES_USER",
    "password": "API_POSTGRES_PASSWORD",
    "database": "API_POSTGRES_DB",
    "port": "API_POSTGRES_PORT",
    "runMigrationsOnStartup": "API_RUN_MIGRATIONS_ON_STARTUP",
    "logging": "API_POSTGRES_LOGGING"
  },
  "postgresGeoApi": {
    "host": "GEO_POSTGRES_HOST",
    "username": "GEO_POSTGRES_USER",
    "password": "GEO_POSTGRES_PASSWORD",
    "database": "GEO_POSTGRES_DB",
    "port": "GEO_POSTGRES_PORT",
    "runMigrationsOnStartup": "GEOPROCESSING_RUN_MIGRATIONS_ON_STARTUP",
    "logging": "GEO_POSTGRES_LOGGING"
  },
  "network": {
    "cors": {
      "origins_extra": "NETWORK_CORS_ORIGINS"
    }
  },
  "auth": {
    "jwt": {
      "secret": "API_AUTH_JWT_SECRET"
    },
    "xApiKey": {
      "secret": "API_AUTH_X_API_KEY"
    }
  },
  "geoprocessing": {
    "url": "GEOPROCESSING_URL"
  },
  "redisApi": {
    "connection": {
      "host": "REDIS_HOST",
      "port": "REDIS_PORT",
      "password": "REDIS_PASSWORD",
      "tls": {
        "host": "REDIS_TLS_HOST"
      }
    }
  },
  "api": {
    "url": "API_SERVICE_URL",
    "daemonListenPort": "API_DAEMON_LISTEN_PORT"
  },
  "storage": {
    "sharedFileStorage": {
      "localPath": "API_SHARED_FILE_STORAGE_LOCAL_PATH"
    }
  },
  "marxan": {
    "inputFiles": {
      "inputDat": {
        "ioSettings": {
          "INPUTDIR": "MARXAN_IO_SETTING_INPUTDIR",
          "PUNAME": "MARXAN_IO_SETTING_PUNAME",
          "SPECNAME": "MARXAN_IO_SETTING_SPECNAME",
          "PUVSPRNAME": "MARXAN_IO_SETTING_PUVSPRNAME",
          "BOUNDNAME": "MARXAN_IO_SETTING_BOUNDNAME",
          "OUTPUTDIR": "MARXAN_IO_SETTING_OUTPUTDIR"
        }
      }
    }
  },
  "fileUploads": {
    "limits": {
      "singleGeometry": "API_UPLOAD_SINGLE_MAX_SIZE",
      "complexGeometryWithoutProperties": "API_UPLOAD_COMPLEX_MAX_SIZE",
      "complexGeometryWithProperties": "API_UPLOAD_COMPLEX_PROPERTIES_MAX_SIZE"
    }
  },
  "sparkpost": {
    "apikey": "SPARKPOST_APIKEY",
    "origin": "SPARKPOST_ORIGIN"
  },
  "application": {
    "baseUrl": "APPLICATION_BASE_URL"
  },
  "passwordReset": {
    "tokenPrefix": "PASSWORD_RESET_TOKEN_PREFIX",
    "expiration": "PASSWORD_RESET_EXPIRATION"
  },
  "signUpConfirmation": {
    "tokenPrefix": "SIGNUP_CONFIRMATION_TOKEN_PREFIX"
  }
}
