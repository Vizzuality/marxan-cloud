{
  "network": {
    "cors": {
      "origins": [
        "http://localhost:3010"
      ]
    }
  },
  "redisApi": {
   "connection": {
     "host": "marxan-redis",
     "port": 6379
     },
   "concurrency": 50
   },
  "jobOptions":{
    "removeOnComplete": 100,
    "removeOnFail": 1000,
    "attempts": 3,
    "timeout": 60000
  },
  "auth": {
    "jwt": {
      "expiresIn": "2h"
    }
  }
}
