{
  "name": "@marxan-cloud/api",
  "version": "0.4.0",
  "description": "API service for the Marxan Cloud platform",
  "author": "Vizzuality <hello@vizzuality.com> (https://vizzuality.com/)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "console": "NODE_CONFIG_DIR=apps/api/config ts-node --files --project tsconfig.json -r tsconfig-paths/register apps/api/src/console.ts",
    "api:start:dev": "nodemon --exec 'NODE_OPTIONS=\"--max_old_space_size=3072\" NODE_CONFIG_DIR=apps/api/config ts-node --files --project tsconfig.json -r tsconfig-paths/register apps/api/src/main.ts' -L | bunyan",
    "geoprocessing:start:dev": "nodemon --exec 'NODE_CONFIG_DIR=apps/geoprocessing/config ts-node --files --project tsconfig.json -r tsconfig-paths/register apps/geoprocessing/src/main.ts' -L | bunyan",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "rm -rf dist && nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit:ci": "jest --silent",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "api:test:e2e": "NODE_OPTIONS=\"--max_old_space_size=3072\" jest --config ./apps/api/test/jest-e2e.json",
    "geoprocessing:test:e2e": "jest --config ./apps/geoprocessing/test/jest-e2e.json",
    "ts-coverage": "typescript-coverage-report",
    "api:typeorm": "NODE_CONFIG_DIR=apps/api/config ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/api/ormconfig.ts",
    "geoprocessing:typeorm": "NODE_CONFIG_DIR=apps/geoprocessing/config ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/geoprocessing/ormconfig.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  },
  "engines": {
    "node": "~14.18"
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "2.3.1",
    "@greenelab/hclust": "0.0.1",
    "@nestjs-architects/typed-cqrs": "0.1.1",
    "@nestjs/common": "7.6.18",
    "@nestjs/config": "1.1.5",
    "@nestjs/core": "7.6.18",
    "@nestjs/cqrs": "7.0.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@pgtyped/cli": "0.11.0",
    "@pgtyped/query": "0.11.0",
    "@turf/turf": "6.5.0",
    "abort-controller": "3.0.0",
    "archiver": "5.3.0",
    "axios": "0.21.1",
    "bcrypt": "^5.0.0",
    "bullmq": "1.46.0",
    "byline": "5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "commander": "7.2.0",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "fp-ts": "2.10.5",
    "geojson": "0.5.0",
    "helmet": "^4.3.1",
    "jsonapi-serializer": "^3.6.7",
    "lodash": "^4.17.20",
    "mapshaper": "0.5.91",
    "ms": "2.1.3",
    "multer": "^1.4.2",
    "multistream": "4.1.0",
    "nestjs-base-service": "0.6.0",
    "nestjs-console": "5.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "8.7.1",
    "pg-large-object": "2.0.0",
    "pg-query-stream": "4.2.1",
    "pure-geojson-validation": "0.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sparkpost": "2.1.4",
    "stronger-typed-streams": "0.2.0",
    "swagger-ui-express": "^4.1.6",
    "temp-dir": "^2.0.0",
    "tiny-types": "1.16.1",
    "typeorm": "0.2.30",
    "unzipper": "^0.10.11",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@mapbox/vector-tile": "1.3.1",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/archiver": "5.1.0",
    "@types/axios": "0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/byline": "4.2.33",
    "@types/config": "^0.0.38",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.5",
    "@types/fs-extra": "9.0.11",
    "@types/geojson": "7946.0.7",
    "@types/http-proxy": "^1.17.5",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.167",
    "@types/ms": "0.7.31",
    "@types/multer": "^1.4.5",
    "@types/multistream": "2.1.1",
    "@types/node": "^14.17.27",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pbf": "^3.0.2",
    "@types/pg-large-object": "2.0.4",
    "@types/puppeteer": "^5.4.5",
    "@types/sparkpost": "^2.1.5",
    "@types/supertest": "^2.0.10",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "axios-mock-adapter": "1.19.0",
    "bunyan": "^1.8.14",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "10.0.0",
    "geojson2shp": "0.4.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "nock": "13.1.0",
    "nodemon": "^2.0.6",
    "pbf": "^3.2.1",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typed-emitter": "1.3.1",
    "typescript": "^4.2.2",
    "typescript-coverage-report": "^0.4.0",
    "utility-types": "^3.10.0",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@marxan-api/(.*)": "<rootDir>/apps/api/src/$1",
      "@marxan-geoprocessing/(.*)": "<rootDir>/apps/geoprocessing/src/$1",
      "@marxan/utils/(.*)": "<rootDir>/libs/utils/src/$1",
      "@marxan/utils": "<rootDir>/libs/utils/src",
      "@marxan/scenarios-planning-unit/(.*)": "<rootDir>/libs/scenarios-planning-unit/src/$1",
      "@marxan/scenarios-planning-unit": "<rootDir>/libs/scenarios-planning-unit/src",
      "@marxan/admin-regions/(.*)": "<rootDir>/libs/admin-regions/src/$1",
      "@marxan/admin-regions": "<rootDir>/libs/admin-regions/src",
      "@marxan/api-events/(.*)": "<rootDir>/libs/api-events/src/$1",
      "@marxan/api-events": "<rootDir>/libs/api-events/src",
      "@marxan-jobs/planning-unit-geometry/(.*)": "<rootDir>/libs/planning-unit-geometry/src/$1",
      "@marxan-jobs/planning-unit-geometry": "<rootDir>/libs/planning-unit-geometry/src",
      "@marxan-jobs/planning-unit-protection-level/(.*)": "<rootDir>/libs/planning-unit-protection-level/src/$1",
      "@marxan-jobs/planning-unit-protection-level": "<rootDir>/libs/planning-unit-protection-level/src",
      "@marxan/scenario-cost-surface/(.*)": "<rootDir>/libs/scenario-cost-surface/src/$1",
      "@marxan/scenario-cost-surface": "<rootDir>/libs/scenario-cost-surface/src",
      "@marxan/marxan-input/(.*)": "<rootDir>/libs/marxan-input/src/$1",
      "@marxan/marxan-input": "<rootDir>/libs/marxan-input/src",
      "@marxan/marxan-output/(.*)": "<rootDir>/libs/marxan-output/src/$1",
      "@marxan/marxan-output": "<rootDir>/libs/marxan-output/src",
      "@marxan/planning-area-repository/(.*)": "<rootDir>/libs/planning-area-repository/src/$1",
      "@marxan/planning-area-repository": "<rootDir>/libs/planning-area-repository/src",
      "@marxan/scenario-puvspr/(.*)": "<rootDir>/libs/scenario-puvspr/src/$1",
      "@marxan/scenario-puvspr": "<rootDir>/libs/scenario-puvspr/src",
      "@marxan/scenario-run-queue/(.*)": "<rootDir>/libs/scenario-run-queue/src/$1",
      "@marxan/scenario-run-queue": "<rootDir>/libs/scenario-run-queue/src",
      "@marxan/features/(.*)": "<rootDir>/libs/features/src/$1",
      "@marxan/features": "<rootDir>/libs/features/src",
      "@marxan/geofeature-calculations/(.*)": "<rootDir>/libs/geofeature-calculations/src/$1",
      "@marxan/geofeature-calculations": "<rootDir>/libs/geofeature-calculations/src",
      "@marxan/iucn/(.*)": "<rootDir>/libs/iucn/src/$1",
      "@marxan/iucn": "<rootDir>/libs/iucn/src",
      "@marxan/shapefile-converter/(.*)": "<rootDir>/libs/shapefile-converter/src/$1",
      "@marxan/shapefile-converter": "<rootDir>/libs/shapefile-converter/src",
      "@marxan/planning-units-grid/(.*)": "<rootDir>/libs/planning-units-grid/src/$1",
      "@marxan/planning-units-grid": "<rootDir>/libs/planning-units-grid/src",
      "@marxan/protected-areas/(.*)": "<rootDir>/libs/protected-areas/src/$1",
      "@marxan/protected-areas": "<rootDir>/libs/protected-areas/src",
      "@marxan/tiles/(.*)": "<rootDir>/libs/tiles/src/$1",
      "@marxan/tiles": "<rootDir>/libs/tiles/src",
      "@marxan/planning-unit-features/(.*)": "<rootDir>/libs/planning-unit-features/src/$1",
      "@marxan/planning-unit-features": "<rootDir>/libs/planning-unit-features/src",
      "@marxan/projects/(.*)": "<rootDir>/libs/projects/src/$1",
      "@marxan/projects": "<rootDir>/libs/projects/src",
      "@marxan/blm-calibration/(.*)": "<rootDir>/libs/blm-calibration/src/$1",
      "@marxan/blm-calibration": "<rootDir>/libs/blm-calibration/src",
      "@marxan/files-repository/(.*)": "<rootDir>/libs/files-repository/src/$1",
      "@marxan/files-repository": "<rootDir>/libs/files-repository/src",
      "@marxan/cloning/(.*)": "<rootDir>/libs/cloning/src/$1",
      "@marxan/cloning": "<rootDir>/libs/cloning/src",
      "@marxan/code-guide/(.*)": "<rootDir>/libs/code-guide/src/$1",
      "@marxan/code-guide": "<rootDir>/libs/code-guide/src"
    }
  }
}
