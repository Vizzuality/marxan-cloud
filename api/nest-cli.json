{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "root": "apps/api",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "geoprocessing": {
      "type": "application",
      "root": "apps/geoprocessing",
      "entryFile": "main",
      "sourceRoot": "apps/geoprocessing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/geoprocessing/tsconfig.app.json"
      }
    },
    "scenarios-planning-unit": {
      "type": "library",
      "root": "libs/scenarios-planning-unit",
      "entryFile": "index",
      "sourceRoot": "libs/scenarios-planning-unit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/scenarios-planning-unit/tsconfig.lib.json"
      }
    }
  }
}