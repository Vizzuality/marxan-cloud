{
  "monorepo": true,
  "root": "apps/main-api",
  "sourceRoot": "apps/main-api/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main-api/tsconfig.app.json"
  },
  "projects": {
    "main-api": {
      "type": "application",
      "root": "apps/main-api",
      "entryFile": "main",
      "sourceRoot": "apps/main-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main-api/tsconfig.app.json"
      }
    },
    "geoprocessing": {
      "type": "application",
      "root": "apps/geoprocessing",
      "entryFile": "main",
      "sourceRoot": "apps/geoprocessing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/geoprocessing/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}
