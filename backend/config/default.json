{
  "network": {
    "cors": {
      "origins": [
        "http://localhost:3010"
      ]
    }
  },
  "redisApi": {
    "concurrency": 50,
    "limiter": {
      "max": 10,
      "duration": 1000
    },
    "connection": {
      "host": "marxan-redis",
      "port": "6379"
    }
  },
  "jobOptions":{
    "removeOnComplete": 100,
    "removeOnFail": 1000,
    "attempts": 3,
    "timeout": 60000
  },
  "auth": {
    "jwt": {
      "expiresIn": "2h"
    }
  },
  "geoprocessing": {
    "url": "http://geoprocessing:3000"
  }
}

